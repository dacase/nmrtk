#
#  input file for the GeneticAlgorithmFit to find multi-structure solution from RDC data      
#
populationSize: 25            // how many individuals in the population
pMutation: 0.001              // likelihood of mutating new offspring 
pCrossover: 0.9               // likelihood of crossing over parents
gaSteps: 1000000              // maximum number of Genetic steps
nPrints: 1000                 // number of steps for printing
gaSeed:  11234                // seed for GA random number generator
minError: 0.05                // minimum R factor for stop simulation 
isRandomErr: 1                // add random noise to Exp NMR data > 0 for Yes  
nmrSeed: 15497                // seed for generating random errors for NMR data 
sigmaErr: 0.1                 // variance of random noise of NMR data 
nmrExpFile: RDCs_exp.dat      // file store experimental NMR data
numNMRPoints:  15             // total number of NMR data points
nmrConfFile: RDCs_sorted.dat  // pre-calculated nmr data from unique structures
numConfs:  1026               // total number of unique structures 
rankCtrl: R                   // rank control, 'D': RMSD, 'N': normalized RMSD, "R": R factor
randomMethod: 0               // random method for weighting
